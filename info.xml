<?xml version="1.0"?>
<extension key="org.project60.membership" type="module">
  <file>membership</file>
  <name>Membership</name>
  <description>Project60 extension to make CiviMember more flexible.</description>
  <license>AGPLv3</license>
  <maintainer>
    <author>B. Endres</author>
    <email>info@systopia.de</email>
  </maintainer>
  <releaseDate>2022-04-01</releaseDate>
  <version>0.7-beta1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <urls>
    <url desc="Main Extension Page">https://github.com/Project60/org.project60.membership</url>
    <url desc="Documentation">https://github.com/Project60/org.project60.membership</url>
    <url desc="Support">https://github.com/Project60/org.project60.membership/issues/new</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <comments/>
  <civix>
    <namespace>CRM/Membership</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Membership_Upgrader</upgrader>
</extension>
